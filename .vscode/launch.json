{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "jdk",
      "request": "attach",
      "processId": "${command:jdk.java.attachDebugger.pickProcess}"
    },
    {
      "name": "Attach to Port",
      "type": "jdk",
      "request": "attach",
      "hostName": "localhost",
      "port": "8000"
    },
    {
      "type": "jdk",
      "request": "launch",
      "name": "Launch Java App"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/garv-rahut/Programs/OODP",
      "program": "/home/garv-rahut/Bootcamp 2.0/Week 2 lvl2",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}